{
    "name": "chrisoprea/blade-mingcute-icons",
    "description": "A package to easily make use of MingCute icons in your Laravel Blade views.",
    "keywords": ["Blade", "MingCute Icons", "Laravel"],
    "homepage": "https://github.com/chrisoprea/blade-mingcute-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Oprea"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisOprea\\MingCute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisOprea\\MingCuteI\\BladeMingCuteIconsServiceProvider.php"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}